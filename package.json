{
  "name": "yuefou",
  "scripts": {
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "start:engine": "dotenv -- nodemon -e ts,graphql -x ts-node src/engine.ts",
    "dev": "npm-run-all --parallel start playground",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "playground": "graphql playground",
    "build": "rimraf dist && tsc",
    "deploy:staging": "git checkout staging && git push && pm2 deploy staging update",
    "prod:deploy": "yarn build && yarn prod:start",
    "prod:start": "pm2 startOrGracefulReload ecosystem.config.js",
    "prod:stop": "pm2 stop ecosystem.config.js",
    "prod:restart": "pm2 restart ecosystem.config.js"
  },
  "dependencies": {
    "@types/node": "^9.6.2",
    "apollo-cache-control": "^0.1.0",
    "apollo-engine": "^1.0.4",
    "bcryptjs": "2.4.3",
    "cos-nodejs-sdk-v5": "^2.3.8",
    "graphql-yoga": "1.5.0",
    "jsonwebtoken": "8.2.0",
    "mkdirp": "^0.5.1",
    "node-uuid": "^1.4.8",
    "prisma-binding": "1.5.16",
    "ramda": "^0.25.0",
    "tslint": "^5.9.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.1",
    "@types/ramda": "^0.25.20",
    "dotenv-cli": "1.4.0",
    "graphql-cli": "2.15.8",
    "nodemon": "1.17.1",
    "npm-run-all": "4.1.2",
    "prisma": "1.3.5",
    "rimraf": "^2.6.2",
    "ts-node": "4.1.0",
    "typescript": "2.7.2"
  },
  "author": {
    "name": "Lyon Wu",
    "email": "littlelyon@aliyun.com"
  },
  "license": "MIT"
}
